  "license": "ISC",
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@playwright/test": "1.34.3",
    "dd-trace": "^3.14.1"
  },
  "repository": "https://github.com/777abhi/playwright-typescript-basics.git"
