{
  "name": "playwright-starter",
  "version": "1.0.0",
  "description": "https://playwright.dev/docs/intro",
  "main": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS=\"dd-trace/ci/init\" npx playwright test --project=chromium",
    "test:sanity": "npx playwright test --project=chromium  -g \"sanity\"",
    "test:regression": "npx playwright test --project=chromium -g \"regression\"",
    "test:newFeature": "npx playwright test --project=chromium -g \"newFeature\"",
    "test:api": "npx playwright test --project=chromium -g \"api\"",
    "test:report": "npx playwright show-report",
    "test:debug": "npx playwright test --project=chromium --debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "dd-trace": "^3.14.1"
  }
}
